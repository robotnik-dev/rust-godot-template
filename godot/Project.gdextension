[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.3
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../rust/target/libdebug/rust.so"
linux.release.x86_64 =   "res://../rust/target/librelease/rust.so"
windows.debug.x86_64 =   "res://../rust/target/debug/rust.dll"
windows.release.x86_64 = "res://../rust/target/release/rust.dll"
macos.debug =            "res://../rust/target/libdebug/rust.dylib"
macos.release =          "res://../rust/target/librelease/rust.dylib"
macos.debug.arm64 =      "res://../rust/target/libdebug/rust.dylib"
macos.release.arm64 =    "res://../rust/target/librelease/rust.dylib"
