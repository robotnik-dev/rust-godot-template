[
    {
        "os-name":"Linux-x86_64",
        "id":"linux",
        "runs-on":"ubuntu-latest",
        "target":"x86_64-unknown-linux-gnu",
        "output":"librust.so",
        "command":"both",
        "toolchain":"stable"
    },
    {
        "os-name":"Windows-x86_64",
        "id":"windows",
        "runs-on":"windows-latest",
        "target":"x86_64-pc-windows-msvc",
        "output":"rust.dll",
        "command":"both",
        "toolchain":"stable"
    },
    {
        "os-name":"macOS-x86_64",
        "id":"mac",
        "runs-on":"macOS-latest",
        "target":"x86_64-apple-darwin",
        "output":"librust.dylib",
        "command":"both",
        "toolchain":"stable"
    },
    {
        "os-name":"Linux-x86_64",
        "id":"web",
        "runs-on":"ubuntu-latest",
        "target":"wasm32-unknown-emscripten",
        "output":"rust.wasm",
        "command":"build",
        "toolchain":"nightly",
        "args":"-Zbuild-std"
    },
    {
        "os-name":"Linux-x86_64",
        "id":"android",
        "runs-on":"aarch64-linux-android",
        "target":"x86_64-unknown-linux-gnu",
        "output":"rust.so",
        "command":"build",
        "toolchain":"stable"
    }
]
